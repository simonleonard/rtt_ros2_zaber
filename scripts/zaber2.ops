import("rtt_ros2")
import("rtt_ros2_node")
import("rtt_ros2_topics")
import("rtt_ros2_geometry_msgs")
import("rtt_ros2_sensor_msgs")
import("rtt_ros2_std_msgs")
# import("rtt_ros2_primitives_typekit")
import("rtt_ros2_zaber")

loadComponent("zaber2", "rtt_ros2_zaber2" )
setActivity("zaber2", 0.01, 0, ORO_SCHED_OTHER)

stream("zaber2.joint_state", ros.topic("/joint_state", false))
stream("zaber2.set_joint_state", ros.topic("/set_joint_state", false))

zaber2.configure() 
zaber2.start() 